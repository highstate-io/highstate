{
  "compilerOptions": {
    "outDir": "bin",
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noCheck": true
  },
  "files": [
    "auth.ts",
    "config/index.ts",
    "config/vars.ts",
    "getKey.ts",
    "getKeyRange.ts",
    "getPrefixRangeEnd.ts",
    "index.ts",
    "key.ts",
    "keyPrefix.ts",
    "provider.ts",
    "rangeScopedState.ts",
    "role.ts",
    "synchronizedDirectory.ts",
    "synchronizedKeyPrefixes.ts",
    "types/index.ts",
    "types/input.ts",
    "types/output.ts",
    "user.ts",
    "utilities.ts"
  ]
}
