{
  "name": "@highstate/cilium",
  "version": "0.11.5",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./unit": "./dist/unit/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "highstate build",
    "update-charts": "../../scripts/update-charts.sh ./assets/charts.json",
    "generate-crds": "./scripts/generate-crds.sh",
    "typecheck": "tsgo --noEmit --skipLibCheck",
    "biome": "biome check --write --unsafe --error-on-warnings",
    "biome:check": "biome check --error-on-warnings"
  },
  "dependencies": {
    "@highstate/cilium-crds": "workspace:^0.10.0",
    "@highstate/common": "workspace:^0.10.0",
    "@highstate/contract": "workspace:^0.10.0",
    "@highstate/k8s": "workspace:^0.10.0",
    "@highstate/library": "workspace:^0.10.0",
    "@highstate/pulumi": "workspace:^0.10.0",
    "@kubernetes/client-node": "^1.3.0",
    "@pulumi/command": "^1.0.2",
    "@pulumi/kubernetes": "^4.18.0",
    "@pulumi/pulumi": "^3.198.0",
    "remeda": "^2.21.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@highstate/cli": "workspace:^0.10.0",
    "@typescript/native-preview": "^7.0.0-dev.20250920.1"
  },
  "repository": {
    "url": "https://github.com/highstate-io/highstate"
  }
}
