{
  "name": "{{packageName}}",
  "private": true,
  {{#if isYarn}}
  "packageManager": "yarn@4.12.0",
  {{/if}}
  {{#if isPnpm}}
  "packageManager": "pnpm@10.26.1",
  {{/if}}
  {{#unless isPnpm}}
  "workspaces": ["packages/*"],
  {{/unless}}
  "dependencies": {
    "@highstate/contract": "{{platformVersion}}",
    "@highstate/pulumi": "{{platformVersion}}",
    "@highstate/library": "{{libraryVersion}}"
  },
  "devDependencies": {
    "@highstate/cli": "{{platformVersion}}",
    "typescript": "^5.9.3"
  }
}
