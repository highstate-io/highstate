{
  "name": "@highstate/api",
  "version": "0.13.2",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./instance.v1": {
      "types": "./src/_generated/instance.v1.ts",
      "default": "./dist/_generated/instance.v1.js"
    },
    "./worker.v1": {
      "types": "./src/_generated/worker.v1.ts",
      "default": "./dist/_generated/worker.v1.js"
    },
    "./secret.v1": {
      "types": "./src/_generated/secret.v1.ts",
      "default": "./dist/_generated/secret.v1.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "highstate build",
    "generate-sdk": "./scripts/generate-sdk.sh",
    "typecheck": "tsgo --noEmit --skipLibCheck",
    "biome": "biome check --write --unsafe --error-on-warnings",
    "biome:check": "biome check --error-on-warnings"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@highstate/cli": "workspace:*",
    "@typescript/native-preview": "^7.0.0-dev.20250920.1",
    "ts-proto": "^2.7.5"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.0",
    "long": "^5.3.2",
    "nice-grpc-common": "^2.0.2",
    "protobufjs": "^7.5.3"
  },
  "repository": {
    "url": "https://github.com/highstate-io/highstate"
  }
}
