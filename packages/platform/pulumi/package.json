{
  "name": "@highstate/pulumi",
  "version": "0.15.0",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "highstate": {
    "sourceHash": {
      ".": {
        "mode": "manual",
        "version": "1"
      }
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "tags": [
      "platform"
    ]
  },
  "scripts": {
    "build": "highstate build",
    "typecheck": "tsgo --noEmit --skipLibCheck",
    "biome": "biome check --write --unsafe --error-on-warnings",
    "biome:check": "biome check --error-on-warnings"
  },
  "dependencies": {
    "@highstate/contract": "workspace:*",
    "@pulumi/pulumi": "^3.198.0",
    "deepmerge-ts": "^7.1.5",
    "remeda": "^2.21.0",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@highstate/cli": "workspace:*",
    "@typescript/native-preview": "^7.0.0-dev.20250920.1"
  },
  "repository": {
    "url": "https://github.com/highstate-io/highstate"
  }
}
