{
  "name": "create-highstate",
  "version": "0.16.0",
  "description": "The scaffolding tool for the Highstate project.",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/highstate-io/highstate"
  },
  "exports": {
    "./package.json": "./package.json"
  },
  "bin": {
    "create-highstate": "./dist/main.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "tags": [
      "platform"
    ]
  },
  "scripts": {
    "build": "highstate build",
    "typecheck": "tsgo --noEmit --skipLibCheck",
    "biome": "biome check --write --unsafe --error-on-warnings",
    "biome:check": "biome check --error-on-warnings"
  },
  "dependencies": {
    "@highstate/cli": "workspace:*",
    "clipanion": "^4.0.0-rc.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@typescript/native-preview": "^7.0.0-dev.20250920.1"
  }
}
